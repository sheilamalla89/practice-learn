package oop;

public class BankAccount {
	
	String accType;
	String accNumber;
	String routingNumber;
	String accHolderName;
	String balance;
	
	public BankAccount() {
		accType = "Checking";
		accNumber = "510534900";
		routingNumber = "530023423";
		accHolderName = "Dummy";
	}
	
	public BankAccount(String accType, String accNumber, String routingNumber, String accHolderName) {
		this.accType = accType;
		this.accNumber = accNumber;
		this.routingNumber = routingNumber;
		this.accHolderName = accHolderName;
		
	}
	
	public void deposit(String depositAmount) {
		System.out.println("Total deposited amount: $"+depositAmount);
	}
	
	public void withdrawn(String withdrawAmt) {
		System.out.println("Total withdrawn amount is $"+withdrawAmt);
	}
	
	public String checkBalance() {
		return 
	}
	

}
