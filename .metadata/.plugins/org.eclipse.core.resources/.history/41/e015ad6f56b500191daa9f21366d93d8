package oop;

public class AccountApp {
	public static void main(String[] args) {
		LoanAccount la = new LoanAccount();
		la.increaseRate();
		la.setRate();
		la.setMortSchedule();
		la.setTerm(20);
	// Polymorphism changes where we are pointing
		
		IRate loanAccount = new LoanAccount();
		loanAccount.increaseRate();
		loanAccount.setRate();
		
	}

}
