package basics;

public class FibonnaciApp {

	public static void main(String[] args) {
		
	
	int firstNumber = 0;
	int secondNumber = 1;
	=
	System.out.println(firstNumber+" "+secondNumber);
	int third;
	int number = 10;
	int i;
	for(i = 2;i<number;++i) {
		third = firstNumber+secondNumber;
		System.out.print(" "+third);
		firstNumber = secondNumber;
		secondNumber = third;
		
		
	}
	}
}
