		acc1.accType = "Saving";
		acc1.accNumber = "1234567890";
		
		acc2.accHolderName = "Test2";
		acc2.accType = "Checking";
		acc2.accNumber = "0987654321";
		
		acc1.toString();
	//	acc2.toString();
	}
}
